{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Zeon Docs",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#0E62FE",
    "light": "#0E62FE",
    "dark": "#0E62FE",
    "anchors": {
      "from": "#0D9373",
      "to": "#07C983"
    }
  },
  "topbarLinks": [
    {
      "name": "Go Back Home",
      "url": "https://zeonhq.com"
    }
  ],
  "topbarCtaButton": {
    "type": "github",
    "url": "https://github.com/zeon-hq/zeon-docs"
  },
  "tabs": [
    {
      "name": "Roadmap",
      "url": "essentials"
    },
    {
      "name": "Blog",
      "url": "https://www.google.com"
    },
    {
      "name": "Discord",
      "url": "https://www.google.com"
    }
  ],
  "modeToggle": {
    "default": "dark",
    "isHidden": true
  },
  "navigation": [
    {
      "group": "Get Started",
      "pages": ["introduction", "quickstart", "development"]
    },
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation"
      ]
    }
  ],
  "footerSocials": {
    "twitter": "https://twitter.com/mintlify",
    "github": "https://github.com/mintlify",
    "linkedin": "https://www.linkedin.com/company/mintsearch"
  }
}
